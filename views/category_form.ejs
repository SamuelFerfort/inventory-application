<h1><%= title %></h1>
<form method="post">
  <label for="name">Name: </label>
  <input
    type="text"
    name="name"
    id="name"
    placeholder="Seafood, Dairy..."
    required
    min="1"
    max="30"
    value="<%= category ? category.name : '' %>"
  />
  <button type="submit">Submit</button>
</form>
